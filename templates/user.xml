<templateSet group="user">
  <template name="bi" value="::Kernel.binding.pry" description="Adding binding.pry" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="RUBY" value="true" />
    </context>
  </template>
  <template name="by" value="byebug" shortcut="TAB" description="Adding byebug" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="RHTML" value="true" />
      <option name="RUBY" value="true" />
    </context>
  </template>
  <template name="log" value="Rails.logger.debug(&quot;!!!!!!!!!!&quot;)" description="Rails logger" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HAML" value="true" />
      <option name="RHTML" value="true" />
      <option name="RUBY" value="true" />
    </context>
  </template>
  <template name="cy" value="data-cypress=&quot;$END$&quot;" description="data-cypress" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="bii" value="&lt;% ::Kernel.binding.pry %&gt;" description="Adding binding.pry" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HAML" value="true" />
      <option name="HTML" value="true" />
      <option name="RHTML" value="true" />
    </context>
  </template>
</templateSet>